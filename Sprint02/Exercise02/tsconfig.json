{
  "compilerOptions": {
    /* Language and Environment */
    "target": "es5",
    "lib": ["ES5", "ES7", "DOM"],

    /* Modules */
    "module": "commonjs",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,

    /* Type Checking */
    "strict": false,
    "skipLibCheck": true
  }
}
